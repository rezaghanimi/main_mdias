<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="FunencListTab">
        <div class="layui-tab layui-tab-brief" lay-filter="list_tab">
          <ul class="layui-tab-title">
              <t t-foreach='tabs' t-as="tab_data">
                   <li t-att-data-tab-key="tab_data.key">
                        <t t-esc="tab_data.name"/>
                    </li>
              </t>
          </ul>
        </div>
    </t>
</templates>