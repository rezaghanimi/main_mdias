<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="funenc_uinstall_extend_from" model="ir.ui.view">
            <field name="name">funenc_uinstall</field>
            <field name="model">base.module.uninstall</field>
            <field name="inherit_id" ref="base.view_base_module_uninstall"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='module_id']" position="after">
                    <label for="pass_word" string="卸载密码:" />
                    <field name="pass_word"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>