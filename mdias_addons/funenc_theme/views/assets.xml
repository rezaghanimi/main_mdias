        <template id="funenc_theme_res_async" name="funenc_theme_res_async" inherit_id="web.layout">
			<xpath expr="//t[@t-raw]" position="before">
                <script type="application/javascript" src="/funenc_theme/static/layui/layui.js" />
                <link rel="stylesheet" href="/funenc_theme/static/lib/layui/css/layui.css" />
			</xpath>
		</template>

        <template id="fx_select_js_assets" name="fx_select_js_assets" inherit_id="web.assets_common">
            <!-- replace the slect2.js with full select2.js -->
            <xpath expr="//script[@src='/web/static/lib/select2/select2.js']" position="replace">
                <script type="text/javascript" src="/funenc_theme/static/lib/select2/js/select2.full.min.js"/>
            </xpath>
        </template>

        <template id="fx_select_css_assets" name="fx_select_css_assets" inherit_id="web.assets_common">
            <!-- replace the slect2.css -->
            <xpath expr="//link[@href='/web/static/lib/select2/select2.css']" position="replace">
                <link rel="stylesheet" type="text/css" href="/funenc_theme/static/lib/select2/css/select2.min.css"/>
            </xpath>
