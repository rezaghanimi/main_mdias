
<odoo>
    <data>
        
        <record model="ir.ui.view" id="interlock_table.condition_switches_info_list">
            <field name="name">条件道岔</field>
            <field name="model">interlock_table.condition_switches_info</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="switch_id"/>
                    <field name="is_protect"/>
                    <field name="is_reverse"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="interlock_table.condition_switches_info_form">
            <field name="name">条件道岔</field>
            <field name="model">interlock_table.condition_switches_info</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="switch_id">switch_id</label>
                        <div class="col-sm-10">
                            <field name="switch_id"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="is_protect">is_protect</label>
                        <div class="col-sm-10">
                            <field name="is_protect"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="is_reverse">is_reverse</label>
                        <div class="col-sm-10">
                            <field name="is_reverse"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

        
        <record model="ir.actions.act_window" id="interlock_table.condition_switches_info_act_window">
            <field name="name">条件道岔</field>
            <field name="res_model">interlock_table.condition_switches_info</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    


    