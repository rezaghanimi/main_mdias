<odoo>
    <data>

        <record model="ir.ui.view" id="metro_park_interlock_route_list">
            <field name="name">进路</field>
            <field name="model">metro_park.interlock.route</field>
            <field name="arch" type="xml">
                <tree js_class="fnt_table" edit="0" create="0">
                    <field name="index" fixed_left="1"/>
                    <field name="start_rail" fixed_left="1"/>
                    <field name="end_rail" fixed_left="1"/>
                    <field name="route_type"/>
                    <field name="direction"/>
                    <field name="rail_and_switches" />
                    <field name="route_sub_type"/>
                    <field name="method"/>
                    <field name="origin_press_btn"/>
                    <field name="mdias_press_start"/>
                    <field name="mdias_press_end"/>
                    <field name="direction_switch"/>
                    <field name="length"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="metro_park_interlock_route_form">
            <field name="name">进路</field>
            <field name="model">metro_park.interlock.route</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="index">index</label>
                        <div class="col-sm-10">
                            <field name="index"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="route_type">route_type</label>
                        <div class="col-sm-10">
                            <field name="route_type"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="direction">direction</label>
                        <div class="col-sm-10">
                            <field name="direction"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="route_sub_type">route_sub_type</label>
                        <div class="col-sm-10">
                            <field name="route_sub_type"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="method">method</label>
                        <div class="col-sm-10">
                            <field name="method"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="origin_press_btn">origin_press_btn</label>
                        <div class="col-sm-10">
                            <field name="origin_press_btn"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="mdias_press_start">mdias_press_start</label>
                        <div class="col-sm-10">
                            <field name="mdias_press_start"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="mdias_press_end">mdias_press_end</label>
                        <div class="col-sm-10">
                            <field name="mdias_press_end"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="direction_switch">direction_switch</label>
                        <div class="col-sm-10">
                            <field name="direction_switch"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"
                               for="sec_infos">sec_infos</label>
                        <div class="col-sm-10">
                            <field name="sec_infos"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="metro_park_interlock_route_act_window">
            <field name="name">联锁表进路</field>
            <field name="res_model">metro_park.interlock.route</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    