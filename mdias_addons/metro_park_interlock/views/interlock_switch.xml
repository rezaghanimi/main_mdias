<odoo>
    <data>

        <record model="ir.ui.view" id="metro_park_interlock_switch_list">
            <field name="name">联锁道岔信息</field>
            <field name="model">metro_park.interlock.switch</field>
            <field name="arch" type="xml">
                <tree js_class="fnt_table">
                    <field name="index"/>
                    <field name="start"/>
                    <field name="end"/>
                    <field name="is_anti_position"/>
                    <field name="is_single"/>
                    <field name="is_m"/>
                    <field name="hostile_signal_id"/>
                    <field name="train_route_id"/>
                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="metro_park_interlock_switch_form">
            <field name="name">联锁道岔表单</field>
            <field name="model">metro_park.interlock.switch</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="index">index</label>
                        <div class="col-sm-10">
                            <field name="index"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="start">start</label>
                        <div class="col-sm-10">
                            <field name="start"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="end">end</label>
                        <div class="col-sm-10">
                            <field name="end"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="is_anti_position">is_anti_position</label>
                        <div class="col-sm-10">
                            <field name="is_anti_position"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="is_single">is_single</label>
                        <div class="col-sm-10">
                            <field name="is_single"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="is_m">is_m</label>
                        <div class="col-sm-10">
                            <field name="is_m"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="hostile_signal_id">hostile_signal_id</label>
                        <div class="col-sm-10">
                            <field name="hostile_signal_id"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="train_route_id">train_route_id</label>
                        <div class="col-sm-10">
                            <field name="train_route_id"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

    </data>
</odoo>
    