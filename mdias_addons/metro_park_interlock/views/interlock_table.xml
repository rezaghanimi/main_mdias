<odoo>
    <data>

        <record model="ir.ui.view" id="metro_park_interlock_table_list">
            <field name="name">联锁表</field>
            <field name="model">metro_park.interlock.table</field>
            <field name="arch" type="xml">
                <tree js_class="inter_lock_table"
                      options="{'height': 'full-270'}"
                      delete="1"
                      create="0"
                      edit="0">
                    <field name="route_ids" width="200" />
                    <field name="location" width="200" />
                    <field name="file_data" width="200" />
                    <field name="file_name" width="250" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="metro_park_interlock_table_form">
            <field name="name">联锁表</field>
            <field name="model">metro_park.interlock.table</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="route_ids">route_ids</label>
                        <div class="col-sm-10">
                            <field name="route_ids"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="file_data">file_data</label>
                        <div class="col-sm-10">
                            <field name="file_data"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="file_name">file_name</label>
                        <div class="col-sm-10">
                            <field name="file_name"/>
                        </div>
                    </div>

                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="metro_park_interlock_table_act_window">
            <field name="name">联锁表</field>
            <field name="res_model">metro_park.interlock.table</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem action="metro_park_interlock_table_act_window"
                  id="metro_park_interlock_table_menu"
                  name="联锁表"
                  parent="metro_park_interlock.menu_root"/>
    </data>
</odoo>
    