
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_interlock.route_cache_list">
            <field name="name">route_cache list </field>
            <field name="model">interlock_table.route_cache</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="route_type"/>
                    <field name="start_rail"/>
                    <field name="end_rail"/>
                    <field name="route_info"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="metro_park_interlock.route_cache_form">
            <field name="name">route_cache form </field>
            <field name="model">interlock_table.route_cache</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="route_type">route_type</label>
                        <div class="col-sm-10">
                            <field name="route_type"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="start_rail">start_rail</label>
                        <div class="col-sm-10">
                            <field name="start_rail"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="end_rail">end_rail</label>
                        <div class="col-sm-10">
                            <field name="end_rail"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="route_info">route_info</label>
                        <div class="col-sm-10">
                            <field name="route_info"/>
                        </div>
                    </div>
                    
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="metro_park_interlock.route_cache_act_window">
            <field name="name">route_cache</field>
            <field name="res_model">interlock_table.route_cache</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        <menuitem action="metro_park_interlock.route_cache_act_window"
                  id="metro_park_interlock.route_cache_menu"
                  name="进路缓存"
                  parent="metro_park_interlock.menu_root"/>
    </data>
</odoo>
    