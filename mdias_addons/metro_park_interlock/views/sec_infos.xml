
<odoo>
    <data>
        
        <record model="ir.ui.view" id="interlock_table.sec_infos_list">
            <field name="name">sec_infos list </field>
            <field name="model">interlock_table.sec_infos</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="index"/>
                    <field name="sec"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="interlock_table.sec_infos_form">
            <field name="name">sec_infos form </field>
            <field name="model">interlock_table.sec_infos</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="index">index</label>
                        <div class="col-sm-10">
                            <field name="index"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="sec">sec</label>
                        <div class="col-sm-10">
                            <field name="sec"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

    </data>
</odoo>


    