
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_base.time_table_syn_log_list">
            <field name="name">时刻表同步日志</field>
            <field name="model">metro_park_base.time_table_syn_log</field>
            <field name="arch" type="xml">
                <tree js_class="fnt_table" options="{'height':'full-340'}">
                    <field name="no"/>
                    <field name="time"/>
                    <field name="syn_type"/>
                    <field name="syn_status"/>
                    <field name="user"/>
                    <field name="content"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="metro_park_base.time_table_syn_log_form">
            <field name="name">时刻表同步日志</field>
            <field name="model">metro_park_base.time_table_syn_log</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="time_table">time_table</label>
                        <div class="col-sm-10">
                            <field name="time_table"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="no">no</label>
                        <div class="col-sm-10">
                            <field name="no"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="time">time</label>
                        <div class="col-sm-10">
                            <field name="time"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="syn_type">syn_type</label>
                        <div class="col-sm-10">
                            <field name="syn_type"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="syn_status">syn_status</label>
                        <div class="col-sm-10">
                            <field name="syn_status"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="user">user</label>
                        <div class="col-sm-10">
                            <field name="user"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="ip_address">ip_address</label>
                        <div class="col-sm-10">
                            <field name="ip_address"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="metro_park_base.time_table_syn_log_act_window">
            <field name="name">时刻表同步日志</field>
            <field name="res_model">metro_park_base.time_table_syn_log</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    