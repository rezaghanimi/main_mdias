<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!--  自动同步施工信息, 由于用户需要配置时间，所以放到这里  -->
        <record id="ir_cron_construction_dispatch_sync" model="ir.cron">
            <field name="name">定时同步施工信息</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
            <field name="model" eval="metro_park_dispatch.construction_dispatch" />
            <field name="function" eval="sync_construction_dispatch" />
        </record>

    </data>
</odoo>
