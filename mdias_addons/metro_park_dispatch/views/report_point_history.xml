
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_dispatch.report_point_history_list">
            <field name="name">report_point_history list </field>
            <field name="model">metro_park_dispatch.report_point_history</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="report_time"/>
                    <field name="rail"/>
                    <field name="train"/>
                    <field name="report_type"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="metro_park_dispatch.report_point_history_form">
            <field name="name">report_point_history form </field>
            <field name="model">metro_park_dispatch.report_point_history</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="report_time">report_time</label>
                        <div class="col-sm-10">
                            <field name="report_time"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="rail">rail</label>
                        <div class="col-sm-10">
                            <field name="rail"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="train">train</label>
                        <div class="col-sm-10">
                            <field name="train"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="report_type">report_type</label>
                        <div class="col-sm-10">
                            <field name="report_type"/>
                        </div>
                    </div>
                    
                </form>
            </field>
        </record>

        
        <record model="ir.actions.act_window" id="metro_park_dispatch.report_point_history_act_window">
            <field name="name">report_point_history</field>
            <field name="res_model">metro_park_dispatch.report_point_history</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    