
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_dispatch.plan_import_wizard_list">
            <field name="name">plan_import_wizard list </field>
            <field name="model">metro_park_dispatch.plan_import_wizard</field>
            <field name="arch" type="xml">
                <tree js_class="fnt_table" >
                    <field name="file_data"/>
                    <field name="file_name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="metro_park_dispatch.plan_import_wizard_form">
            <field name="name">plan_import_wizard form </field>
            <field name="model">metro_park_dispatch.plan_import_wizard</field>
            <field name="arch" type="xml">
                <form>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="file_name">file_name</label>
                        <div class="col-sm-10">
                            <field name="file_name"/>
                        </div>
                    </div>
                    
                </form>
            </field>
        </record>

        
        <record model="ir.actions.act_window" id="metro_park_dispatch.plan_import_wizard_act_window">
            <field name="name">plan_import_wizard</field>
            <field name="res_model">metro_park_dispatch.plan_import_wizard</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    