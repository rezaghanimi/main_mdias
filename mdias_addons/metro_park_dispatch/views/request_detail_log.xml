
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_dispatch.request_detail_log_list">
            <field name="name">request_detail_log list </field>
            <field name="model">metro_park_dispatch.request_detail_log</field>
            <field name="arch" type="xml">
                <tree js_class="fnt_table">
                    <field name="operation_time"/>
                    <field name="worker"/>
                    <field name="content"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="metro_park_dispatch.request_detail_log_form">
            <field name="name">request_detail_log form </field>
            <field name="model">metro_park_dispatch.request_detail_log</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="operation_time">operation_time</label>
                        <div class="col-sm-10">
                            <field name="operation_time"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="worker">worker</label>
                        <div class="col-sm-10">
                            <field name="worker"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="content">content</label>
                        <div class="col-sm-10">
                            <field name="content"/>
                        </div>
                    </div>
                    
                </form>
            </field>
        </record>

        
        <record model="ir.actions.act_window" id="metro_park_dispatch.request_detail_log_act_window">
            <field name="name">request_detail_log</field>
            <field name="res_model">metro_park_dispatch.request_detail_log</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!--<menuitem action="metro_park_dispatch.request_detail_log_act_window" -->
                  <!--id="metro_park_dispatch.request_detail_log_menu" -->
                  <!--name="调车申请日志" -->
                  <!--parent="metro_park_dispatch.menu_root"/>-->
    </data>
</odoo>
    