<odoo>
    <data>
        <record model="ir.ui.view" id="switch_log_tree_id">
            <field name="name">交换机日志</field>
            <field name="model">maintenance_management.switch_log</field>
            <field name="arch" type="xml">
                <tree create="0">
                    <field name="log_time" width="150"/>
                    <field name="log_model" width="150"/>
                    <field name="log_class" width="150"/>
                    <field name="log_symbol" width="300"/>
                    <field name="log_contents" width="300"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="switch_log_form_id">
            <field name="name">交换机日志</field>
            <field name="model">maintenance_management.switch_log</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="log_time">日志时间</label>
                        <div class="col-sm-10">
                            <field name="log_time"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="log_model">日志模块</label>
                        <div class="col-sm-10">
                            <field name="log_model"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="log_class">日志级别</label>
                        <div class="col-sm-10">
                            <field name="log_class"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="log_symbol">日志助记符</label>
                        <div class="col-sm-10">
                            <field name="log_symbol"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="log_contents">日志内容</label>
                        <div class="col-sm-10">
                            <field name="log_contents"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>
    </data>
</odoo>