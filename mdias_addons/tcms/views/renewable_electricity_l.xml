<odoo>
    <data>
        <record model="ir.ui.view" id="renewable_electricity_l_tree_id">
            <field name="name">再生电量</field>
            <field name="model">renewable_electricity_l</field>
            <field name="arch" type="xml">
                <tree create="0">
                    <field name="name"/>
                    <field name="electricity"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="renewable_electricity_l_form_id">
            <field name="name">再生电量</field>
            <field name="model">renewable_electricity_l</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="electricity"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="renewable_electricity_l_window_id">
            <field name="name">再生电量</field>
            <field name="res_model">renewable_electricity_l</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem action="renewable_electricity_l_window_id"
                  parent="tcms_tcms_act_menu_parent_id"
                  id="renewable_electricity_l_menu_id"
                  name="再生电量"/>
    </data>
</odoo>