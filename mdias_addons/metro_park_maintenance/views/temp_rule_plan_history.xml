
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_maintenance.temp_rule_plan_history_list">
            <field name="name">temp_rule_plan_history list </field>
            <field name="model">metro_park_maintenance.temp_rule_plan_history</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="temp_rule_id"/>
                    <field name="rule_info_id"/>
                    <field name="dev"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="metro_park_maintenance.temp_rule_plan_history_form">
            <field name="name">temp_rule_plan_history form </field>
            <field name="model">metro_park_maintenance.temp_rule_plan_history</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="temp_rule_id">temp_rule_id</label>
                        <div class="col-sm-10">
                            <field name="temp_rule_id"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="rule_info_id">rule_info_id</label>
                        <div class="col-sm-10">
                            <field name="rule_info_id"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="dev">dev</label>
                        <div class="col-sm-10">
                            <field name="dev"/>
                        </div>
                    </div>
                    
                </form>
            </field>
        </record>

        
        <record model="ir.actions.act_window" id="metro_park_maintenance.temp_rule_plan_history_act_window">
            <field name="name">temp_rule_plan_history</field>
            <field name="res_model">metro_park_maintenance.temp_rule_plan_history</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    