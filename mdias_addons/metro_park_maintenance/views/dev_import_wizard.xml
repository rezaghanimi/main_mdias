
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_maintenance.dev_import_wizard_list">
            <field name="name">设备导入 </field>
            <field name="model">metro_park_maintenance.dev_import_wizard</field>
            <field name="arch" type="xml">
                <tree js_class="fnt_table">
                    <field name="xls_file"/>
                    <field name="file_name"/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="metro_park_maintenance.dev_import_wizard_form">
            <field name="name">设备导入</field>
            <field name="model">metro_park_maintenance.dev_import_wizard</field>
            <field name="arch" type="xml">
                <form>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="xls_file">xls_file</label>
                        <div class="col-sm-10">
                            <field name="xls_file"/>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="file_name">file_name</label>
                        <div class="col-sm-10">
                            <field name="file_name"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="metro_park_maintenance.dev_import_wizard_act_window">
            <field name="name">设备导入</field>
            <field name="res_model">metro_park_maintenance.dev_import_wizard</field>
            <field name="view_mode">tree,form</field>
            <field name="target">new</field>
        </record>

    </data>
</odoo>
