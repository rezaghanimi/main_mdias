
<odoo>
    <data>
        
        <record model="ir.ui.view" id="metro_park_maintenance.export_produce_plan_wizard_list">
            <field name="name">export_produce_plan_wizard list </field>
            <field name="model">metro_park_maintenance.export_produce_plan_wizard</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="end_month"/>
                    <field name="start_year"/>
                    <field name="start_month"/>
                    <field name="end_year"/>
                </tree>
            </field>
        </record>

        
        <record model="ir.ui.view" id="metro_park_maintenance.export_produce_plan_wizard_form">
            <field name="name">export_produce_plan_wizard form </field>
            <field name="model">metro_park_maintenance.export_produce_plan_wizard</field>
            <field name="arch" type="xml">
                <form>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="start_year">start_year</label>
                        <div class="col-sm-10">
                            <field name="start_year"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="start_month">start_month</label>
                        <div class="col-sm-10">
                            <field name="start_month"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="end_year">end_year</label>
                        <div class="col-sm-10">
                            <field name="end_year"/>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" for="end_month">end_month</label>
                        <div class="col-sm-10">
                            <field name="end_month"/>
                        </div>
                    </div>

                    <footer>
                        <button class="btn btn-success" type="object" name="on_ok" text="确定" />
                        <button class="btn btn-info" text="取消" special="cancel" />
                    </footer>
                    
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="metro_park_maintenance.export_produce_plan_wizard_act_window">
            <field name="name">export_produce_plan_wizard</field>
            <field name="res_model">metro_park_maintenance.export_produce_plan_wizard</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>
    